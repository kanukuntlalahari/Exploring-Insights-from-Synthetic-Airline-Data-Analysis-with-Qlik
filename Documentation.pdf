	function isDemoValidLink(input) {
		var urlPattern = /^(https?:\/\/)?[^\s]*$/i;
		return urlPattern.test(input);
	}

	function uploadVideoLink() {
		var enrollID = $('#enroId').val();
		var videoLink = $('#videoLink').val();
		if (videoLink != "") {
			var onp = isDemoValidLink(videoLink);
			if (onp == true) {
				alertify.confirm('Once the demo video link is submitted, it cannot be modified.', function() {
					$.ajax({
						url: base_url + "Student_API/uploadDemoVideoLink",
						method: "POST",
						data: {
							'enrollID': enrollID,
							'videoLink': videoLink
						},
						success: function(data) {
							data = $.parseJSON(data);
							if (data.succ == '1') {
								$('#videoPopupDiv').modal('hide');
								alertify.success('Link uploaded successfully !!');
								setTimeout(() => {
									location.reload()
								}, 2000)
							}
						}
					});
				})
			} else {
				alertify.error('Invalid Link Format');
			}
		} else {
			alertify.error('Please upload Link!');
		}
	}

	function isValidGithubURL(url) {
		const pattern = /^https:\/\/github\.com\/.*/;
		return pattern.test(url);
	}

	function uploadGitLink() {
		var enrollID = $('#enlId').val();
		var gitLink = $('#gitLink').val();
