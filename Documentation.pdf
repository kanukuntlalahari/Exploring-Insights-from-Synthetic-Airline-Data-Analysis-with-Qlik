    <script>
      function readStudentNotificationStatus() {
        $.ajax({
          url: base_url + "Student_API/updateReadNotifications",
          data: '',
          type: 'POST',
          processData: false,
          contentType: false,
          success: function(data) {
            var data = $.parseJSON(data);
            if (data.success == 1) {
              getUnReadStudentNotificationCount();
            }
          }
        })
      }

      function getUnReadStudentNotificationCount() {
        $.ajax({
          url: base_url + "Student_API/getUnReadStudentNotificationsCount",
          data: '',
          type: 'POST',
          processData: false,
          contentType: false,
          success: function(data) {
            var data = $.parseJSON(data);
            if (data.success == 1) {
              // alert(data.count);
              $('#studentCount').html(data.count);
            }
          }
        })
      }
    </script>

